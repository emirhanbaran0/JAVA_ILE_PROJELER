package Characters;

public class Character {

	private String name;
	private int id;
	private boolean active;
	
	public Character(String name,int id)  
	{
		this.name=name;
		this.id=id;
		this.active=true;
	}
	
	public String getName()
	{
		return this.name;
	}
	
	public int getId()
	{
		return this.id;
	}
	
	public boolean getActive()
	{
		return this.active;
	}
	
	public void setActive(boolean active)
	{
		this.active=active;
	}
}
